IOConfig=2A0-2AF(3FF::),2E0-2EF(3FF::),2F0-2FF(3FF::),300-30F(3FF::)
MEMConfig=A0000-A3fff,C0000-C3fff,C4000-C7fff,CC000-Cffff,D0000-D3fff,DC000-Dffff,E0000-E3fff

;****************************************************************************
