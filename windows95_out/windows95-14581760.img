%pcmcia.DeviceDesc%=pcmcia.ndi, PCMCIA\SMC-PCM_ETHERNET_ADAPTER-67E0    ;NDIS3 SMC 


[ARCNET]
%*pnp8324.DeviceDesc%=*pnp8324.ndi, *pnp8324 ;ARCNET Compatible

[PUREDATA]
%*pnp81e6.DeviceDesc%=*pnp81e6.ndi, *pnp81e6 ;Pure Data PDI508+ (ArcNet)
%*pnp81e7.DeviceDesc%=*pnp81e7.ndi, *pnp81e7 ;Pure Data PDI516+ (ArcNet)

[DELL]
%*Dell.DeviceDesc%=*Dell.ndi, *pnp8228       ;Dell Lattitude XP port replicator

;****************************************************************************
; pcmcia    SMC 8016 PCMCIA
;****************************************************************************
[pcmcia.ndi]
AddReg=pcmcia.ndi.reg,SMC1.ndi.reg

[pcmcia.ndi.reg]
HKR,Ndi,DeviceID,,"SMC-PCM_ETHERNET_ADAPTER-67E0"


;****************************************************************************
; SMC9000 
;****************************************************************************

[*Smc9000.ndi]
AddReg    = Smc9000.ndi.reg,Smc9000.Gen.Resources,Smc9000.Gen.Params,*Smc9000.DevID
LogConfig = Smc9000.LogConfig

[*Dell.ndi]
AddReg    = Smc9000.ndi.reg,Smc9000.Gen.Resources,Smc9000.Gen.Params,*Dell.DevID
LogConfig = Dell.LogConfig

[*Smc9000.DevID]
HKR,Ndi,DeviceID,,"*SMC9000"

[*Dell.DevID]
HKR,Ndi,DeviceID,,"*PNP8228"

[Smc9000.LogConfig]
ConfigPriority = HARDRECONFIG
IOConfig       = 20@200-3FF%FFE0(3FF::)
IRQConfig      = 9,3,10,11
 
[Dell.LogConfig]
ConfigPriority = HARDRECONFIG
IOConfig       = 20@300-3FF%FFE0
IRQConfig      = 10

;****************************************************************************
; ARCNET Compatible
;****************************************************************************
[*pnp8324.ndi]
AddReg=*pnp8324.DevID,ARC.Gen.Resources,SMCARC.ndi.reg,MC_Channel
LogConfig=*pnp8324.LogConfig
[*pnp8324.DevID]
