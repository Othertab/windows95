"2"="10 Mbps Ethernet"
"3"="100 Mbps Ethernet"
"4"="100/10 Mbps Ethernet"
"5"="100 Mbps VG AnyLAN"
"6"="4 Mbps Token-Ring"
"7"="16 Mbps Token-Ring"
"8"="16/4 Mbps Token-Ring"
"9"="2 Mbps Arcnet"
"10"="20 Mbps Arcnet"
"11"="FDDI"
"12"="ATM"
"13"="AppleTalk"

[HKEY_LOCAL_MACHINE\DesktopManagement\Install\Defaults\MIF\Net\%ComponentID\7\2]
"Name"="Transmission Capability"
"Description"="Transmission capability of the adapter."
"Access"="Read-Only"
"Storage"="Common"
"Type"="Enum"
":Value"="HKLM\\DesktopManagement\\DATA\\%ComponentID\\NetworkAdapterHardwareExtra\\TransmissionCapability"

[HKEY_LOCAL_MACHINE\DesktopManagement\Install\Defaults\MIF\Net\%ComponentID\7\2\Enum]
"1"="Normal"
"2"="Full Duplex"

[HKEY_LOCAL_MACHINE\DesktopManagement\Install\Defaults\MIF\Net\%ComponentID\7\3]
"Name"="Network Adapter RAM Size"
"Description"="This is the total size of the adapter RAM in Kb."
"Access"="Read-Only"
"Storage"="Common"
"Type"="Integer"
":Value"="HKLM\\DesktopManagement\\DATA\\%ComponentID\\NetworkAdapterHardwareExtra\\NetworkAdapterRAMSize"

[HKEY_LOCAL_MACHINE\DesktopManagement\Install\Defaults\MIF\Net\%ComponentID\7\4]
"Name"="Bus Type"
"Description"="This is the type of bus used to interface the adapter to the host."
"Access"="Read-Only"
"Storage"="Common"
"Type"="Enum"
":Value"="HKLM\\DesktopManagement\\DATA\\%ComponentID\\NetworkAdapter802Port\\BusType"
"!Value"="B;IN(0,1)(1,1)(2,2)(3,4)(4,3)(5,5)(6,7)"

[HKEY_LOCAL_MACHINE\DesktopManagement\Install\Defaults\MIF\Net\%ComponentID\7\4\Enum]
