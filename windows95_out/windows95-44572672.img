"2"="ISA"
"3"="EISA"
"4"="MCA"
"5"="PCI"
"6"="VL"
"7"="PCMCIA"
"8"="Parallel"
"9"="NEC98"
"256"="Motherboard"

[HKEY_LOCAL_MACHINE\DesktopManagement\Install\Defaults\MIF\Net\%ComponentID\7\5]
"Name"="Bus Width"
"Description"="This is the width (in bits) of the data path used to interface the adapter to the host."
"Access"="Read-Only"
"Storage"="Common"
"Type"="Enum"
":Value"="HKLM\\DesktopManagement\\DATA\\%ComponentID\\NetworkAdapter802Port\\BusWidth"
"!Value"="B;IN(0,2)(8,3)(16,4)(32,5)(64,6)(128,7)"

[HKEY_LOCAL_MACHINE\DesktopManagement\Install\Defaults\MIF\Net\%ComponentID\7\5\Enum]
"1"="Other"
"2"="Unknown"
